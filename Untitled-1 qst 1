#include <stdio.h>

int somaMatriz3x3(int matriz[3][3]) {
    int soma = 0;
    for(int i = 0; i < 3; i++) {
        for(int j = 0; j < 3; j++) {
            soma += matriz[i][j];
        }
    }
    return soma;
}

int main() {
    int mat[3][3] = {{1,2,3},{4,5,6},{7,8,9}};
    printf("Soma da matriz 3x3: %d\n", somaMatriz3x3(mat));
    return 0;
}
